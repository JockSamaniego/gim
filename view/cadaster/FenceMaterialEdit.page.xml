<?xml version="1.0" encoding="UTF-8"?>
<page xmlns="http://jboss.org/schema/seam/pages"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://jboss.org/schema/seam/pages http://jboss.org/schema/seam/pages-2.3.xsd"
      no-conversation-view-id="/FenceMaterialList.xhtml"
      login-required="true">

   <begin-conversation join="true" flush-mode="MANUAL"/>

   <action execute="#{fenceMaterialHome.wire}"/>

   <param name="fenceMaterialFrom"/>
   <param name="fenceMaterialId" value="#{fenceMaterialHome.fenceMaterialId}"/>


   <navigation from-action="#{fenceMaterialHome.persist}">
      <rule if-outcome="persisted">
         <end-conversation/>
         <redirect view-id="/cadaster/FenceMaterialList.xhtml"/>
      </rule>
   </navigation>

   <navigation from-action="#{fenceMaterialHome.update}">
      <rule if-outcome="updated">
         <end-conversation/>
         <redirect view-id="/cadaster/FenceMaterialList.xhtml"/>
      </rule>
   </navigation>

   <navigation from-action="#{fenceMaterialHome.remove}">
      <rule if-outcome="removed">
         <end-conversation/>
         <redirect view-id="/cadaster/FenceMaterialList.xhtml"/>
      </rule>
   </navigation>

</page>
