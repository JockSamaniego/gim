<?xml version="1.0" encoding="UTF-8"?>
<page xmlns="http://jboss.org/schema/seam/pages"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://jboss.org/schema/seam/pages http://jboss.org/schema/seam/pages-2.3.xsd"
      no-conversation-view-id="/BusinessList.xhtml"
      login-required="true">

   <begin-conversation join="true" flush-mode="MANUAL"/>   

   <param name="unbuiltLotFrom"/>   
   
   <navigation from-action="#{unbuiltLotHome.createCopy()}">
      <rule if-outcome="ready">
         <end-conversation/>
         <redirect view-id="/cadaster/UnbuiltLotList.xhtml"/>
      </rule>
      <rule if-outcome="failed">         
         <redirect view-id="/cadaster/CreateCopyUnbuiltLots.xhtml"/>
      </rule>
   </navigation>

</page>
