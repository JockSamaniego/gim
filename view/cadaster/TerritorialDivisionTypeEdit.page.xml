<?xml version="1.0" encoding="UTF-8"?>
<page xmlns="http://jboss.org/schema/seam/pages"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://jboss.org/schema/seam/pages http://jboss.org/schema/seam/pages-2.3.xsd"
      no-conversation-view-id="/cadaster/TerritorialDivisionTypeList.xhtml"
      login-required="true">

   <begin-conversation join="true" flush-mode="MANUAL"/>

   <action execute="#{territorialDivisionTypeHome.wire}"/>

   <param name="territorialDivisionTypeFrom"/>
   <param name="territorialDivisionTypeId" value="#{territorialDivisionTypeHome.territorialDivisionTypeId}"/>


   <navigation from-action="#{territorialDivisionTypeHome.persist}">
      <rule if-outcome="persisted">
         <end-conversation/>
         <redirect view-id="/cadaster/TerritorialDivisionTypeList.xhtml"/>
      </rule>
   </navigation>

   <navigation from-action="#{territorialDivisionTypeHome.update}">
      <rule if-outcome="updated">
         <end-conversation/>
         <redirect view-id="/cadaster/TerritorialDivisionTypeList.xhtml"/>
      </rule>
   </navigation>

   <navigation from-action="#{territorialDivisionTypeHome.remove}">
      <rule if-outcome="removed">
         <end-conversation/>
         <redirect view-id="/cadaster/TerritorialDivisionTypeList.xhtml"/>
      </rule>
   </navigation>

</page>
