<?xml version="1.0" encoding="UTF-8"?>
<page xmlns="http://jboss.org/schema/seam/pages"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
      xsi:schemaLocation="http://jboss.org/schema/seam/pages http://jboss.org/schema/seam/pages-2.3.xsd" 
      login-required="true">
      
    <begin-conversation join="true" flush-mode="MANUAL"/>
    
    <action execute="#{rightsTransferHome.wire}"/>

   <param name="rightsTransferFrom"/>
   <param name="rightsTransferId" value="#{rightsTransferHome.rightsTransferId}"/>
    
  <navigation from-action="#{rightsTransferHome.persist()}">
      <rule if-outcome="persisted">
         <end-conversation/>
         <redirect view-id="/cadaster/RightsTransferList.xhtml"/>
      </rule>
   </navigation>
   
  <navigation from-action="#{rightsTransferHome.update()}">
      <rule if-outcome="updated">
         <end-conversation/>
         <redirect view-id="/cadaster/RightsTransferList.xhtml"/>
      </rule>
   </navigation>
   
</page>


