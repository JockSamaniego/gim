<p:table xmlns="http://www.w3.org/1999/xhtml"
	xmlns:ui="http://java.sun.com/jsf/facelets"
	xmlns:f="http://java.sun.com/jsf/core"
	xmlns:p="http://jboss.com/products/seam/pdf"
	columns="5"
	widthPercentage="100" 
	spacingAfter="10" 
	widths="4 3 3 3 3">
	
	<f:facet name="defaultCell">
		<p:cell borderWidth="0.2" borderColor="#000000" />
	</f:facet>

	<p:cell colspan="5" horizontalAlignment="center" grayFill="0.90">
		<p:font size="7" style="bold">
			<p:paragraph alignment="center">#{messages['receipt.depositsDetail']}</p:paragraph>
		</p:font>
	</p:cell>


	<p:cell horizontalAlignment="center" grayFill="0.90">
		<p:font size="6" style="bold">
			<p:paragraph alignment="center">#{messages['deposit.date']}</p:paragraph>
		</p:font>
	</p:cell>
	<p:cell horizontalAlignment="center" grayFill="0.90">
		<p:font size="6" style="bold">
			<p:paragraph alignment="center">#{messages['deposit.capital']}</p:paragraph>
		</p:font>
	</p:cell>
	<p:cell horizontalAlignment="center" grayFill="0.90">
		<p:font size="6" style="bold">
			<p:paragraph alignment="center">#{messages['deposit.interest']}</p:paragraph>
		</p:font>
	</p:cell>
	<p:cell horizontalAlignment="center" grayFill="0.90">
		<p:font size="6" style="bold">
			<p:paragraph alignment="center">#{messages['deposit.value']}</p:paragraph> 
		</p:font>
	</p:cell>
	<p:cell horizontalAlignment="center" grayFill="0.90">
		<p:font size="6" style="bold">
			<p:paragraph alignment="center">#{messages['deposit.balance']}</p:paragraph>
		</p:font>
	</p:cell>
	
	<p:font size="6">
	<ui:repeat var="_dep" value="#{_deposit.municipalBond.deposits}">
		<p:cell horizontalAlignment="center">
			<p:paragraph>
				<p:text value="#{_dep.date}">
					<f:convertDateTime pattern="#{messages['common.datePattern']}" />
				</p:text>
			</p:paragraph>
		</p:cell>
		<p:cell horizontalAlignment="right">
			<p:paragraph>
				<p:text value="#{_dep.capital}">
					<f:convertNumber maxFractionDigits="#{messages['common.maxFractionDigits']}" minFractionDigits="#{messages['common.maxFractionDigits']}"/>
				</p:text>
			</p:paragraph>
		</p:cell>
		<p:cell horizontalAlignment="right">
			<p:paragraph>
				<p:text value="#{_dep.interest}">
					<f:convertNumber maxFractionDigits="#{messages['common.maxFractionDigits']}" minFractionDigits="#{messages['common.maxFractionDigits']}"/>
				</p:text>
			</p:paragraph>
		</p:cell>
		<p:cell horizontalAlignment="right">
			<p:paragraph>
				<p:text value="#{_dep.value}">
					<f:convertNumber maxFractionDigits="#{messages['common.maxFractionDigits']}" minFractionDigits="#{messages['common.maxFractionDigits']}"/>
				</p:text>
			</p:paragraph>
		</p:cell>
		<p:cell horizontalAlignment="right">
			<p:paragraph>
				<p:font size="7" style="bold">
				<p:text value="#{_dep.balance}">
					<f:convertNumber maxFractionDigits="#{messages['common.maxFractionDigits']}" minFractionDigits="#{messages['common.maxFractionDigits']}"/>
				</p:text>
				</p:font>
			</p:paragraph>
		</p:cell>
	</ui:repeat>
	</p:font>
	
	<p:font size="7" style="bold">
	<p:cell colspan="4" horizontalAlignment="right">
			<p:paragraph alignment="center">#{messages['receipt.taxesToPay']}</p:paragraph>		
	</p:cell>
	<p:cell horizontalAlignment="right">	
		<p:paragraph>		
			<p:text value="#{_deposit.municipalBond.taxesTotal}">
				<f:convertNumber maxFractionDigits="#{messages['common.maxFractionDigits']}" minFractionDigits="#{messages['common.maxFractionDigits']}"/>
			</p:text>
		</p:paragraph> 
	</p:cell>
	</p:font>
</p:table>

