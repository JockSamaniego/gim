<?xml version="1.0" encoding="UTF-8"?>
<page xmlns="http://jboss.org/schema/seam/pages"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://jboss.org/schema/seam/pages http://jboss.org/schema/seam/pages-2.3.xsd"
      no-conversation-view-id="/income/InterestRateList.xhtml"
      login-required="true">

   <begin-conversation join="true" flush-mode="MANUAL"/>

   <action execute="#{interestRateHome.wire}"/>

   <param name="interestRateFrom"/>
   <param name="interestRateId" value="#{interestRateHome.interestRateId}"/>


   <navigation from-action="#{interestRateHome.save()}">
      <rule if-outcome="persisted">
         <end-conversation/>
         <redirect view-id="/income/InterestRateList.xhtml"/>
      </rule>
      <rule if-outcome="updated">
         <end-conversation/>
         <redirect view-id="/income/InterestRateList.xhtml"/>
      </rule>
      <rule if-outcome="failed">         
         <redirect view-id="/income/InterestRateEdit.xhtml"/>
      </rule>
   </navigation>

   <navigation from-action="#{interestRateHome.remove}">
      <rule if-outcome="removed">
         <end-conversation/>
         <redirect view-id="/income/InterestRateList.xhtml"/>
      </rule>
   </navigation>

</page>
