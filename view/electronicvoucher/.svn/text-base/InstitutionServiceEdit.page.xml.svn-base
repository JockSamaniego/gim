<?xml version="1.0" encoding="UTF-8"?>
<page xmlns="http://jboss.com/products/seam/pages" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://jboss.com/products/seam/pages http://jboss.com/products/seam/pages-2.2.xsd"
	no-conversation-view-id="/market/MarketList.xhtml" login-required="true">

	<begin-conversation join="true" flush-mode="MANUAL" />

	<action execute="#{institutionServiceHome.wire}" />
 
	<param name="institutionServiceId" value="#{institutionServiceHome.institutionServiceId}" />
	<navigation from-action="#{institutionServiceHome.persist}">
		<rule if-outcome="persisted">
			<end-conversation />
			<redirect view-id="/electronicvoucher/InstitutionServiceList.xhtml" />
		</rule>
	</navigation>
	<navigation from-action="#{institutionServiceHome.update}">
		<rule if-outcome="updated">
			<end-conversation />
			<redirect view-id="/electronicvoucher/InstitutionServiceList.xhtml" />
		</rule>
	</navigation>

	<navigation from-action="#{institutionServiceHome.remove}">
		<rule if-outcome="removed">
			<end-conversation />
			<redirect view-id="/electronicvoucher/InstitutionServiceList.xhtml" />
		</rule>
	</navigation>

</page>
