<?xml version="1.0" encoding="UTF-8"?>
<page xmlns="http://jboss.org/schema/seam/pages"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://jboss.org/schema/seam/pages http://jboss.org/schema/seam/pages-2.3.xsd"
      no-conversation-view-id="/binnaclecrv/BinnacleCRVList.xhtml"
      login-required="true">
      
  
   <begin-conversation join="true" flush-mode="MANUAL"/>

   <action execute="#{binnacleCRVHome.wireArrival}"/>

   <param name="binnacleCRVFrom"/>
   <param name="binnacleCRVId" value="#{binnacleCRVHome.binnacleCRVId}"/>

   <navigation from-action="#{binnacleCRVHome.updateReArrival}">
      <rule if-outcome="updated">
         <end-conversation/>
         <end-conversation before-redirect="true"/>
         <redirect view-id="/binnaclecrv/BinnacleCRVList.xhtml"/>
      </rule>
   </navigation>

</page>
