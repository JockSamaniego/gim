<?xml version="1.0" encoding="UTF-8"?>
<page login-required="true" xmlns="http://jboss.org/schema/seam/pages"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://jboss.org/schema/seam/pages http://jboss.org/schema/seam/pages-2.3.xsd">
	
	
	<param name="startDate" value="#{notificationHome.startDate}" />
	<param name="endDate" value="#{notificationHome.endDate}" />
	
	<action execute="#{notificationHome.loadValues()}"/>
	
	<navigation from-action="#{notificationHome.generateReport()}">
		<rule if-outcome="readyForPrint">
			<redirect view-id="/coercive/report/NotificationSummaryReport.xhtml"/>
		</rule>	
	</navigation>
</page>
