<?xml version="1.0" encoding="UTF-8"?>
<page xmlns="http://jboss.org/schema/seam/pages"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://jboss.org/schema/seam/pages http://jboss.org/schema/seam/pages-2.3.xsd"
      no-conversation-view-id="/coercive/infractions/InfringementAgreementList.xhtml"
      login-required="true">
      
  
   <begin-conversation join="true" flush-mode="MANUAL"/>

   <action execute="#{infringementAgreementHome.wire}"/>

   <param name="infringementAgreementFrom"/>
   <param name="infringementAgreementId" value="#{infringementAgreementHome.infringementAgreementId}"/>

   <navigation from-action="#{infringementAgreementHome.persist}">
      <rule if-outcome="persisted">
         <end-conversation/>
         <end-conversation before-redirect="true"/>
         <redirect view-id="/coercive/infractions/InfringementAgreementList.xhtml"/>
      </rule>
   </navigation>

   <navigation from-action="#{infringementAgreementHome.update}">
      <rule if-outcome="updated">
         <end-conversation/>
         <end-conversation before-redirect="true"/>
         <redirect view-id="/coercive/infractions/InfringementAgreementList.xhtml"/>
      </rule>
   </navigation>

</page>
