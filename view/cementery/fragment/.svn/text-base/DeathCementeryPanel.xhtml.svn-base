<!DOCTYPE composition PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN"
    "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<ui:composition xmlns="http://www.w3.org/1999/xhtml"
	xmlns:s="http://jboss.com/products/seam/taglib"
	xmlns:ui="http://java.sun.com/jsf/facelets"
	xmlns:f="http://java.sun.com/jsf/core"
	xmlns:h="http://java.sun.com/jsf/html"
	xmlns:a="http://richfaces.org/a4j"
	xmlns:rich="http://richfaces.org/rich"
	xmlns:g="http://gim.gob.ec/jsf" >

	<rich:panel id="deathCementeryPanel"  rendered = "#{unitDeathHome.idDefined}">
		<f:facet name="header">#{messages['cementery.generalData']}</f:facet>

		<s:decorate id="cementeryField" template="../../layout/edit.xhtml" >
			<ui:define name="label">#{messages['cementery.cementery']}:</ui:define>
			<h:inputText value="#{unitDeathHome.instance.section.cementery.name}" disabled="true" />
		</s:decorate>

		<s:decorate id="sectionField" template="../../layout/edit.xhtml" >
			<ui:define name="label">#{messages['cementery.section']}:</ui:define>
			<h:inputText value="#{unitDeathHome.instance.section.name}" disabled="true" />
		</s:decorate>

		<s:decorate id="unitField" template="../../layout/edit.xhtml" >
			<ui:define name="label">#{messages['cementery.section']}:</ui:define>
			<h:inputText value="#{deathHome.instance.unit.code}" disabled="true" />
		</s:decorate>

		<s:decorate id="unitTypeField" template="../../layout/edit.xhtml" >
			<ui:define name="label">#{messages['unit.unitType']}:</ui:define>
			<h:inputText value="#{deathHome.instance.unit.unitType.name}" disabled="true" />
		</s:decorate>

		<div style="clear: both" />
	</rich:panel>
</ui:composition>
