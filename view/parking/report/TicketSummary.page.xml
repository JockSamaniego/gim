<?xml version="1.0" encoding="UTF-8"?>
<page login-required="true" xmlns="http://jboss.org/schema/seam/pages"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://jboss.org/schema/seam/pages http://jboss.org/schema/seam/pages-2.3.xsd">
 
 
  <begin-conversation join="true" flush-mode="MANUAL"/>
  <action execute="#{ticketSummaryList.loadCharge()}"/>
 <param name="parkingLot" value="#{ticketSummaryList.parkingLot}"/> 
 <param name="start" value="#{ticketSummaryList.start}" converterId="org.jboss.seam.ui.DateTimeConverter"/>
 <param name="end" value="#{ticketSummaryList.end}" converterId="org.jboss.seam.ui.DateTimeConverter"/>
 <param name="isOnlyReport" value="#{ticketSummaryList.onlyReport}"/>
 <param name="ticketIds" value="#{ticketSummaryList.ids}"/>

</page>
