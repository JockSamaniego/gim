<?xml version="1.0" encoding="UTF-8"?>
<page xmlns="http://jboss.com/products/seam/pages"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://jboss.com/products/seam/pages http://jboss.com/products/seam/pages-2.2.xsd"
      no-conversation-view-id="/WriteOffTypeList.xhtml"
      login-required="true">

   <begin-conversation join="true" flush-mode="MANUAL"/>

   <action execute="#{writeOffTypeHome.wire}"/>

   <param name="writeOffTypeFrom"/>
   <param name="writeOffTypeId" value="#{writeOffTypeHome.writeOffTypeId}"/>


   <navigation from-action="#{writeOffTypeHome.persist}">
      <rule if-outcome="persisted">
         <end-conversation/>
         <redirect view-id="/WriteOffType.xhtml"/>
      </rule>
   </navigation>

   <navigation from-action="#{writeOffTypeHome.update}">
      <rule if-outcome="updated">
         <end-conversation/>
         <redirect view-id="/WriteOffType.xhtml"/>
      </rule>
   </navigation>

   <navigation from-action="#{writeOffTypeHome.remove}">
      <rule if-outcome="removed">
         <end-conversation/>
         <redirect view-id="/WriteOffTypeList.xhtml"/>
      </rule>
   </navigation>

</page>
