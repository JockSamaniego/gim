<?xml version="1.0" encoding="UTF-8"?>
<page xmlns="http://jboss.org/schema/seam/pages"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://jboss.org/schema/seam/pages http://jboss.org/schema/seam/pages-2.3.xsd" login-required="true">

   <param name="firstResult" value="#{inspectionsList.firstResult}"/>
   <param name="sort" value="#{inspectionsList.orderColumn}"/>
   <param name="dir" value="#{inspectionsList.orderDirection}"/>
   <param name="logic" value="#{inspectionsList.restrictionLogicOperator}"/>

   <param name="from"/>
   <param name="criteria" value="#{inspectionsList.criteria}"/>
   <param name="inspectionsState" value="#{inspectionsList.inspectionsState}"/>
   <param name="inspectionsDate" value="#{inspectionsList.inspectionsDate}" converterId="org.jboss.seam.ui.DateTimeConverter" />
   <param name="name" value="#{inspectionsList.criteriaOwner}" />
   <param name="criteriaNumberTechnicalInformation" value="#{inspectionsList.criteriaNumberTechnicalInformation}" />
   
</page>
