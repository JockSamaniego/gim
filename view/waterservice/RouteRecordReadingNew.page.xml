<?xml version="1.0" encoding="UTF-8"?>
<page login-required="true" no-conversation-view-id="/waterservice/RouteList.xhtml"
	xmlns="http://jboss.org/schema/seam/pages" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://jboss.org/schema/seam/pages http://jboss.org/schema/seam/pages-2.3.xsd">
	<begin-conversation flush-mode="MANUAL" join="true" />
	<action execute="#{routeHome.wire}" />
	<param name="routeFrom" />
	<param name="routeId" value="#{routeHome.routeId}" />

	<navigation from-action="#{routeHome.preEmiteWater()}">
		<rule if-outcome="complete">
			<end-conversation />
			<redirect view-id="/waterservice/RouteList.xhtml" />
		</rule>
	</navigation>
	
	<navigation from-action="#{routeHome.startPreEmission()}">
		<rule if-outcome="complete">
			<end-conversation />
			<redirect view-id="/waterservice/RouteList.xhtml" />
		</rule>
	</navigation>
	
	<navigation from-action="#{routeHome.waterPreEmited()}">
		<rule if-outcome="complete">
			<end-conversation />
			<redirect view-id="/waterservice/RouteList.xhtml" />
		</rule>
	</navigation>

</page>
