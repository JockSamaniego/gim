	private BigDecimal taxesTotal;
	
	private BigDecimal paidTotal;
	
	private BigDecimal taxableTotal;

	/**
	 * Relationships
	 */
		
	@OneToOne(mappedBy="municipalBond", fetch = FetchType.LAZY)
